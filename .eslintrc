{
  "extends": [
    "@antfu",
    "@unocss"
  ],
  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "@typescript-eslint/brace-style": "off",
    "antfu/if-newline": "off",
    "curly": "off",
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "warn",
    "n/prefer-global/process": "off"
  }

}
